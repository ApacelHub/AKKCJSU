local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Apacel Hub | Shockwave Racing üèÅ", HidePremium = false, SaveConfig = true, ConfigFolder = "ApacelWorkspace", IntroText = "Apacel Hub"})

local Tab = Window:MakeTab({
    Name = "Auto Farm ü§ñ",
    PremiumOnly = false
})

Tab:AddButton({
    Name = "ON / Auto Farm (Wins üèÜ)",
    Callback = function()
        OrionLib:MakeNotification({
            Name = "Auto Farm (Wins üèÜ) has started!",
            Content = "",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
        _G.farm = true
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ApacelHub/AKKCJSU/main/a5g7h9j2k4l6"))()
        print("Successful ‚úî (ON)")
    end
})

