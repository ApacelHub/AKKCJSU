local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "AKC Group | Shockwave Racing üèÅ", HidePremium = false, SaveConfig = true, ConfigFolder = "AKC Group Workspace"})
----------------\\\\\\\\\\\\\\\
local Tab = Window:MakeTab({
	Name = "Auto Farm ü§ñ",
	PremiumOnly = false
	})
----------------
	Tab:AddButton({
	Name = "ON / Auto Farm",
	Callback = function()
	    _G.farm = true
	    
	    loadstring(game:HttpGet("https://raw.githubusercontent.com/ApacelHub/AKKCJSU/main/AutoFarmLoop"))()
      		print("Successful ‚úî (ON)")
  	end    
})
----------------
	Tab:AddButton({
	Name = "OFF / Auto Farm",
	Callback = function()
	    _G.farm = false
	    
	    loadstring(game:HttpGet("https://raw.githubusercontent.com/ApacelHub/AKKCJSU/main/AutoFarmLoop"))()
      		print("Successful ‚úî (OFF)")
  	end    
})
----------------\\\\\\\\\\\\\\\
local Tab = Window:MakeTab({
	Name = "Teleport üè¥",
	PremiumOnly = false
	})
----------------\\\\\\\\\\\\\\\
	Tab:AddButton({
	Name = "Fastest Runner",
	Callback = function()
	    wait()
      		local TweenService = game:GetService("TweenService")
local Tw = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out,0,false,0), 
{CFrame = game:GetService("Workspace").Finish.Finish.CFrame}):Play()
      		wait()
      		print("Successful ‚úî (Fastest Runner)")
  	end    
})
----------------\\\\\\\\\\\\\\\
	Tab:AddButton({
	Name = "Boomerang",
	Callback = function()
	    wait()
	    local TweenService = game:GetService("TweenService")
local Tw = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out,0,false,0), 
{CFrame = game:GetService("Workspace").Finish.Finish.CFrame}):Play()
	    wait(6)
	    local TweenService = game:GetService("TweenService")
local Tw = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out,0,false,0), 
{CFrame = game:GetService("Workspace").Spawn.Spawn.CFrame}):Play()
	    wait()
	    print("Successful ‚úî (Boomerang)")
	    end
})
