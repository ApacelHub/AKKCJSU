while _G.loop do
    game:GetService("RunService").Stepped:wait() -- Use RunService's Stepped event for more efficient waiting
    
    for i,v in pairs(game.workspace.Coins.Meshes:GetChildren()) do
        if v.CFrame == CFrame.new(Vector3.new(0, -23.7275181, 0), Vector3.new(0.722326994, 0, -0.691551626), Vector3.new(0, 1, 0), Vector3.new(0.691551626, 0, 0.722326994)) then
            v:Destroy() -- Destroy the object if its CFrame matches
        end
    end
end
